<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.alomapp.alom.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_announcement"
        android:name="com.alomapp.alom.ui.announcement.AnnouncementFragment"
        android:label="@string/title_announcement"
        tools:layout="@layout/fragment_member" />


    <fragment
        android:id="@+id/navigation_members"
        android:name="com.alomapp.alom.ui.members.MembersFragment"
        android:label="@string/title_members" />
    <fragment
        android:id="@+id/navigation_attendance"
        android:name="com.alomapp.alom.attendance"
        android:label="fragment_attendance"
        tools:layout="@layout/fragment_attendance" />
</navigation>